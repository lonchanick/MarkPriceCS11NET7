@page
@using Packt.Shared
@inject NorthwindContext db
@{
    string Orders = "Orders";
    ViewData["Title"] = $"Northwind B2B - {Orders}";
}
<div class="row">
    <h1 class="display-2">@Title</h1>
    @* <p>@Title</p> *@
    <p>
        There are @db.Orders.Count() orders in the northwind data base.
    </p>
</div>